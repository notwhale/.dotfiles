URxvt.font:		xft:xos4 Terminus:pixelsize=20:antialias=true:hinting=true
URxvt.boldFont:		xft:xos4 Terminus:bold:pixelsize=20:antialias=true:hinting=true
URxvt.scrollBar:	false
URxvt.saveLines:	65535
URxvt.perl-ext-common: keyboard-select,url-select,clipboard
URxvt.keysym.M-Escape: perl:keyboard-select:activate
